{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "allowTernary": "off",
    "no-plusplus": "off",
    "no-alert": "off"
  }
}
